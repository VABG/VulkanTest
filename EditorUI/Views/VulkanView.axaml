<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:vm="using:EditorUI.ViewModels"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EditorUI.Views.VulkanView"
             x:DataType="vm:VulkanViewModel"
             Width="{Binding Width}"
             Height="{Binding Height}">
        <ContentControl Content="{Binding VulkanControlContent}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    
</UserControl>
